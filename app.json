{
  "expo": {
    "name": "GyoolTalk",
    "slug": "GyoolTalk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.limsujung.gyoolTalk",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.limsujung.gyoolTalk"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      ["@react-native-seoul/naver-login",
        {
          "urlScheme": "com.limsujung.gyoolTalk"
        }]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "37a13523-0072-462c-aee3-8dc751c763dc"
      },
      "EXPO_PUBLIC_NAVER_CLIENT_ID": "ML4TlZVYL9jxXT14IWVa",
      "EXPO_PUBLIC_NAVER_CLIENT_SECRET": "Iy381KNvZp",
      "EXPO_PUBLIC_NAVER_APP_NAME": "gyooltalk",
      "EXPO_PUBLIC_APP_BUNDLE_IDENTIFIER": "com.limsujung.gyoolTalk"
    }
  }
}
